   "source": [
    "l1 = [8,2,3,0,7]\n",
    "def sum_Of_List(lst, size):\n",
    "    if size == 0:\n",
    "        return 0\n",
    "    else:\n",
    "        return lst[size - 1] + sum_Of_List(lst, size - 1)\n",
    "\n",
    "total = sum_Of_List(l1, len(l1))\n",
    "\n",
    "print(total)"
   ]
